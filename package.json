    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.4.7",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.26",
    "graphql": "^16.12.0",
    "input-otp": "^1.4.2",
