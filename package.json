    "next": "15.4.10",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "payload": "3.69.0",
    "pino": "^9.14.0",
    "react": "19.2.1",
    "react-countup": "^6.5.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.1",
    "react-feather": "^2.0.10",
